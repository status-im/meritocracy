{
  "contracts": ["contracts/**"],
  "app": {
    "css/app.css": ["app/css/**"],
    "js/app.js": ["app/js/index.js"],
    "images/": ["app/images/**"],
    "index.html": "app/index.html"
  },
  "buildDir": "dist/",
  "config": "config/",
  "versions": {
    "solc": "0.5.0",
    "ipfs-api": "17.2.4"
  },
  "plugins": {
    "embarkjs-connector-web3": {},
    "embark-solc": {
      "outputBinary": false
    }
  },
  "options": {
    "solc": {
      "optimize": true,
      "optimize-runs": 200
    }
  }
}
